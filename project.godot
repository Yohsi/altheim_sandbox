"class": "CardType",
"language": "GDScript",
"path": "res://scripts/CardType.gd"
}, {
"base": "Control",
"class": "CardView",
"language": "GDScript",
"path": "res://scripts/CardView.gd"
}, {
"base": "Reference",
"class": "DeckList",
"language": "GDScript",
"path": "res://scripts/DeckList.gd"
}, {
"base": "Node",
"class": "EasyLANNetworkManager",
